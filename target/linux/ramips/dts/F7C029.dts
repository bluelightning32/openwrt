/dts-v1/;

#include "rt5350.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "belkin,f7c029", "ralink,rt5350-soc";
	model = "Belkin F7C029";

	aliases {
		serial1 = &uart;
		led-boot = &led_status_orange;
		led-failsafe = &led_status_orange;
		led-running = &led_status_orange;
		led-upgrade = &led_status_orange;
	};

	leds {
		compatible = "gpio-leds";

		status {
			label = "f7c029:green:status";
			gpios = <&gpio0 0 GPIO_ACTIVE_LOW>;
		};

		power {
			label = "f7c029:green:power";
			gpios = <&gpio0 1 GPIO_ACTIVE_LOW>;
		};

		led_status_orange: orange {
			label = "f7c029:orange:status";
			gpios = <&gpio0 19 GPIO_ACTIVE_LOW>;
		};

		relay {
			label = "f7c029:device:relay";
			gpios = <&gpio0 18 GPIO_ACTIVE_HIGH>;
		};
	};

	keys {
		compatible = "gpio-keys-polled";
		poll-interval = <20>;

		top {
			label = "top";
			gpios = <&gpio0 17 GPIO_ACTIVE_LOW>;
			linux,code = <BTN_0>;
		};

		touch {
			label = "touch";
			gpios = <&gpio0 20 GPIO_ACTIVE_LOW>;
			linux,code = <BTN_1>;
		};
	};
};

&spi0 {
	status = "okay";

	m25p80@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <10000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x0 0x30000>;
				read-only;
			};

			partition@30000 {
				label = "u-boot-env";
				reg = <0x30000 0x10000>;
				read-only;
			};

			factory: partition@40000 {
				label = "factory";
				reg = <0x40000 0x10000>;
				read-only;
			};

			partition@50000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x50000 0x770000>;
			};

			partition@7c0000 {
				label = "firmware2";
				reg = <0x7c0000 0x770000>;
				read-only;
			};

			partition@f30000 {
				label = "belkin_settings";
				reg = <0xf30000 0xa0000>;
				read-only;
			};

			partition@fd0000 {
				label = "unknown";
				reg = <0xfd0000 0x10000>;
				read-only;
			};

			partition@fe0000 {
				label = "nvram";
				reg = <0xfe0000 0x10000>;
				read-only;
			};

			partition@ff0000 {
				label = "user_factory";
				reg = <0xff0000 0x10000>;
				read-only;
			};
		};
	};
};

&wmac {
	ralink,mtd-eeprom = <&factory 0>;
};

&pinctrl {
	state_default: pinctrl0 {
		gpio {
			ralink,group = "i2c", "jtag";
			ralink,function = "gpio";
		};

		uartf {
			ralink,group = "uartf";
			ralink,function = "gpio uartf";
		};
	};
};

&ethernet {
	status = "disabled";
};

&esw {
	status = "disabled";
};

&uart {
	status = "ok";
};
